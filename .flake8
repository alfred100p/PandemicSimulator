[flake8]
exclude =
  # Not python scripts or autogenerated.
  *.sh,
  __pycache__,
  *.pyc,
  *.md,
  *_pb2.py,
  *_pb2_grpc.py,
  matplotlibrc,
  .git/*,
  build/*,
  docs/*,

filename =
  *.py,

ignore = 
  F901,
  W503,
  E226,
  F541,
  E126,
  W504,
  E241,
  E741,
  E127,


max-line-length = 120
copyright-check = True
copyright-regexp = Confidential, Copyright 20\d{2}, Sony Corporation of America, All rights reserved\.
copyright-min-file-size = 500
